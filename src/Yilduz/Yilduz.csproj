<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>

    <Title>QQBotNet.Core</Title>
    <Authors>Zaitonn</Authors>
    <Description>Web api and nodejs module api implementation for Jint</Description>
    <Version>0.0.1</Version>
    <Copyright>Zaitonn</Copyright>
    <RepositoryUrl>https://github.com/Zaitonn/Yilduz</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <IsPublishable>true</IsPublishable>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Jint" />
    <None Include="README.md" Pack="true" PackagePath="/"/>
  </ItemGroup>
</Project>